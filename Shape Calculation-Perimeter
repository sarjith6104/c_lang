#include <stdio.h>

int main() {
    int side, length, width, radius, base1, side_val, base2;
    float pi = 3.14159;

    scanf("%d", &side);
    scanf("%d", &length);
    scanf("%d", &width);
    scanf("%d", &radius);
    scanf("%d", &base1);
    scanf("%d", &side_val);
    scanf("%d", &base2);

    printf("Perimeter of Square:%d\n", 4 * side);
    printf("Perimeter of Rectangle:%d\n", 2 * (length + width));
    printf("Perimeter of Circle:%.2f\n", 2 * pi * radius);
    printf("Perimeter of Triangle:%d\n", base1 + side_val + base2);

    return 0;
}
