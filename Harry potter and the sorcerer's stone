#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
 int num;
    scanf("%d", &num);
    int firstDigit = num / 1000;
    int lastDigit = num % 10;    
    int result = firstDigit + lastDigit;
    printf("%d\n", result);
    return 0;
}
