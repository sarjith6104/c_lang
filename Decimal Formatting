#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float n;
    
    scanf("%f",&n);
    int a=floor(n);
    printf("%.5f\n",n);
    printf("%.4f\n",n);
    printf("%.3f\n",n);
    printf("%.2f\n",n);
    printf("%.1f\n",n);
    printf("%d",a);


    return 0;
}
